			if (avto[allscans[i]]) {
				avtos[avtos.length] = avto[allscans[i]];
			} else {
				avtos[avtos.length] = allscans[i];
			}
		}
	}
	if (avtos.length)
		out += 'Запчасти для ' + avtos.join(', ').toUpperCase() + '. Лучшая Цена, Удобная Доставка по Украине.';
