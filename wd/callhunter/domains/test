	var blockavto = {'Opel': 1, 'Alfa': 1, 'Smart': 1, 'Volvo': 1, 'Fiat': 1, 'Chery': 1, 'Daihatsu': 1, 'Jaguar': 1, 'Chrysler': 1, 'Jeep': 1, 'Land': 1, 'Mg': 1, 'Saab': 1, 'Lada': 1};
